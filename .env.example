# Provider: openai | openrouter
AGENT_PROVIDER=openai

# You can set either OPENAI_API_KEY or OPENROUTER_API_KEY
OPENAI_API_KEY=
OPENROUTER_API_KEY=

# For OpenAI keep default below.
# For OpenRouter it will auto-default to https://openrouter.ai/api/v1 when AGENT_PROVIDER=openrouter
OPENAI_BASE_URL=https://api.openai.com/v1

# Model examples:
# - OpenAI: gpt-4o-mini
# - OpenRouter: openai/gpt-4o-mini or anthropic/claude-3.5-sonnet
AGENT_MODEL=gpt-4o-mini

# Optional OpenRouter attribution headers
OPENROUTER_HTTP_REFERER=
OPENROUTER_X_TITLE=IKOL Agent

AGENT_MEMORY_FILE=agent_memory.json
AGENT_SKILLS_DIR=skills

# Moltbook app key for verifying X-Moltbook-Identity
MOLTBOOK_APP_KEY=
